# Environment variables for Shape Network deployment

# Private key for deployment (DO NOT COMMIT TO VERSION CONTROL)
PRIVATE_KEY=your_wallet_private_key_here

# Shape Network RPC URLs
SHAPE_TESTNET_RPC_URL=https://testnet-rpc.shape.network
SHAPE_MAINNET_RPC_URL=https://mainnet-rpc.shape.network

# Shape Network Explorer API Key (for contract verification)
SHAPE_EXPLORER_API_KEY=your_api_key_here

# OpenAI API Key for AI content generation
OPENAI_API_KEY=your_openai_api_key_here

# Other API keys for AI services
STABILITY_AI_API_KEY=your_stability_ai_api_key_here
HUGGINGFACE_API_KEY=your_huggingface_api_key_here

# Gas reporting
REPORT_GAS=true
